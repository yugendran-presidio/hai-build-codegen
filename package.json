	"c8": {
		"reporter": [
			"lcov",
			"html"
		],
		"exclude": [
			"**/testing-platform/**",
			"**/webview-ui/**",
			"**/.vscode-test/**",
			"**/node_modules/**",
			"node_modules",
			"**/dist-standalone/src/**",
			"**/dist-standalone/vsce-extension/https:/**",
			"**/dist-standalone/vsce-extension/**",
			"**/dist-standalone/https:/**",
			"**/dist-standalone/LIB/src/**",
			"**/dist-standalone/pdfjs-dist/**",
			"**/*.d.ts",
			"**/*.{test,spec}.{js,jsx,ts,tsx,mjs,cjs}",
